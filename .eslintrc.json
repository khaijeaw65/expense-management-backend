{
  "env": {
      "es2021": true,
      "node": true
  },
  "extends": [
    "airbnb-base",
    "airbnb-typescript/base",
    "plugin:@typescript-eslint/eslint-recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "project": "./tsconfig.json"
  },
  "rules": {
    "max-len": [
      "error",
      170
    ],
    "no-underscore-dangle": "off",
    "@typescript-eslint/naming-convention": "off"
  }
}
